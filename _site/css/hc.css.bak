@import url(http://fonts.googleapis.com/css?family=Open+Sans);
/*html5*/
article,aside,dialog,footer,header,section,nav,figure,menu{display:block}

body
{
  color:#2c3e50;
  background-color:#EAEAEF  ;
  width:100%;
  border-top:solid #d90006;
  border-width:10px
}

h1,h2,h3,h4,h5,h6
{
  font-family:"Helvetica Neue", helvetica, "Open Sans", sans-serif;
  color:#000000
}

h1
{
  font-weight:700;
  color:#6d7680
}

h2,h3
{
  font-weight:700
}

h1 a,h2 a,h3 a,h4 a
{
  color:#428fe0
}

h1 a:hover,h2 a:hover,h3 a:hover,h4 a:hover
{
  color:#29598c
}

p
{
  font-family:"Helvetica Neue", helvetica, "Open Sans", sans-serif;
  font-weight:400
}
.Publication-background
{background-image:url(http://lorempixel.com/900/200/abstract);
 position:fixed
 width:90%}


#article
{
  margin-bottom:30px;
  background-color:#f9f9f9;
  box-shadow:1px 1px 0 1px #e8e8e8;
  padding:10px 20px 0
}

.publication-container
{
  width:95%;
  padding-right:0;
}

.Publication-background h1
{  font-size:2.5em;
  font-family:"Helvetica Neue", helvetica, "Open Sans", sans-serif;
  font-weight:600;
  color:#FFFFFF;
	}

.posts
{
  font-size:20px;
  list-style-type:disc;
  font-weight:lighter;
  line-height:200%;
  padding-bottom:30px
}

.container
{
  width:85%;
  padding-right:0;
  padding-top:30px
}

#container-photo1
{   
  margin-left:50px;
  margin-top:30px;    
  height: 500px;
  width: 800px;
  background: url(/images/haidilao2.jpg) no-repeat;
}

#container-photo1:hover
{ 	
  margin-left:50px;
  margin-top:30px;
  background: url(/images/haidilao1.jpg)  no-repeat;
}

#container-photo2
{   
  margin-left:50px;
  margin-top:30px;    
  height: 500px;
  width: 800px;
  background: url(/images/0305.jpg) no-repeat;
}

#container-photo2:hover
{ 	
  margin-left:50px;
  margin-top:30px;
  background: url(/images/03052.jpg)  no-repeat;
}


.container a
{
  border-bottom:1px #8a8a8a dotted;
  text-decoration:none
}

#wrapper
{
  padding-left:18%;
  transition:all .5s ease 0
}

.navbar 
{
  display: none;
}

.navbar a 
{
  text-decoration: none;
  border-bottom: 1px #b1b1b1 solid;
}

#sidebar-wrapper
{
  margin-left:-13.5%;
  left:13.5%;
  width:13.5%;
  background:#7CCD7C;
  position:fixed;
  height:100%;
  overflow-y:auto;
  z-index:1000;
  margin-top:-10px;
  border-top:solid #d90006;
  transition:all .4s ease 0;
  border-width:10px
}

.sidebar-nav
{
  position:absolute;
  top:1px;
  width:100%;
  list-style:none;
  margin:0;
  padding:6.5% 0 6.5% 0;
}

.sidebar-nav li
{
  line-height:40px;
  text-indent:2%;
  font-family:"Helvetica Neue", helvetica, "Open Sans", sans-serif;
  font-weight:600;
  color:#000000;
  padding-left:10%
}

.sidebar-nav li a
{
  color:#3f3f3f;
  display:block;
  text-decoration:none
}

.sidebar-nav li a:hover
{
  color:#FF0000;
  background:#fff;
  text-decoration:none;
  transition:all .2s ease 0
}

.sidebar-nav li a:active,.sidebar-nav li a:focus
{
  text-decoration:none
}

.sidebar-nav > .sidebar-brand
{
  font-size:18px
}

.sidebar-nav > .sidebar-brand > h3
{
  font-size:15px;
  font-weight:400;
  color:#3f3f3f
}

.nav-toggle {
    display: none;
    color: #d90006;
    padding-left: 10px;
    padding-top: 10px;
  }

.brand
{
  font-weight:690;
  color:#3f3f3f;
  font-family:"Helvetica Neue", helvetica, "Open Sans",sans-serif;
  font-size:1.5em;
}

.brand:hover
{
  color:#FFFFF0;
  -o-transition:.5s;
  -ms-transition:.5s;
  -moz-transition:.5s;
  -webkit-transition:.5s;
  transition:.5s
}

.sidebar-nav > .sidebar-brand a
{
  color:#999
}

.sidebar-nav > .sidebar-brand a:hover
{
  color:#fff;
  background:none
}

.content-header
{
  height:50px;
  line-height:50px
}

.content-header h1
{
  line-height:50px;
  display:inline-block;
  margin:0 0 0 10px
}

blockquote
{
  border-left:2px solid #d90006;
  border-right:2px solid #d90006;
  margin-left:-40px;
  margin-right:-40px;
  padding-left:40px;
  background:#f3f3f7
}

ul,ol
{
  font-weight:normal
}

pre
{
  background:#f5f5f8
}

#social-wrapper
{
  color:#5c646f;
  padding:2%,0
}

#social-wrapper a
{
  font-size:14px;
  text-indent:1.5%

}

#social-wrapper li
{
  font-size:14px;
  text-indent:0;
  word-spacing:10px
}

#social-wrapper a:hover
{
  color:#2d1e18;
  transition:all .3s ease 0
}

.showcase-wrapper
{
  width:100%;
  height:80vh;
  min-height:500px;
  margin-top:-50px;
  box-shadow:inset 0 -10px 5px -2px rgba(119,119,119,0.35);
  -moz-box-shadow:inset 0 -10px 5px -2px rgba(119,119,119,0.35);
  -webkit-box-shadow:inset 0 -10px 5px -2px rgba(119,119,119,0.35);
  border-bottom:solid 1px #fff;
  background-size: cover;
  z-index:-4
}

.carousel-caption
{
  font-size:1.5em;
  font-family:"Helvetica Neue", helvetica, "Open Sans", sans-serif;
  font-weight:normal;
  color:#FFFFFF;
}

.showcase-text
{
  font-size:3em;
  font-family:"Helvetica Neue", helvetica, "Open Sans", sans-serif;
  font-weight:bolder;
  color:#FFFFFF;
  text-align:center;
  padding-top:20%
}

.wide
{
  width:100%;
  background-color:#fff;
  margin:0;
  padding:0 10% 10%
}

#welcome-greeting
{
  width:100%;
  padding-top:10px
}

#welcome-greeting > h1
{
  font-size:3em;
  font-weight:700;
  color:#3f3f3f;
  line-height:130%
}

#welcome-greeting h1:hover
{
  font-size:3em;
  font-weight:700;
  color:#2d4440;
  transition:all .5s ease 0
}

#welcome-greeting > h2
{
  font-size:2em;
  font-weight:100;
  color:#2e2e2e;
  padding-top:0;
  line-height:100%
}

#welcome-greeting h2 a
{
  font-weight:100;
  color:#3f3f3f
}

.credit
{
  bottom:0;
  width:100%;
  font-size:.8em;
  text-shadow:1px 1px #fff;
  padding-left:20px;
  margin-bottom:0;
  padding-bottom:5px;
  padding-top:5px;
  margin-top:40px;
  z-index:1;
  color:#cdcdcd
}

.label-danger
{
  background-color:#2d4440;
  font-weight:400;
  border-radius:.2em;
  font-size:120%
}

.label-danger:hover
{
  background-color:#FF6666;
  transition:all .4s ease 0
}

.showcase-button
{
  border-radius:.5em;
  background-color:transparent;
  border:1px solid rgba(255,255,255,.4);
  color:rgba(255,255,255,.8);
  font-weight:200;
  font-family:"Helvetica Neue", helvetica, "Open Sans", sans-serif;
}

.showcase-button:hover
{
  border:2px solid rgba(255,255,255,1);
  color:rgba(255,255,255,1);
  background-color:transparent;
  transition:all .2s ease 0
}


.article-title
{
  font-size:30px;
  font-weight:600;
  color:#004b98;
  padding-top:15px
}

.meta
{
  text-shadow:1px 1px #fff;
  color:#b6b6b6
}

img
{
  max-width:100%;
}


.post
{
  padding-bottom:30px
}

.user-social
{
  color:#c4c4c4;
  margin-right:20px;
  text-decoration:none
}

.user-social a
{
  color:#c4c4c4;
  text-decoration:none;
  margin-right:20px
}

.blue-outline
{
  border:2px solid #d21c38
}

time
{
  color:#0086cc;
  font-family:"Helvetica Neue", helvetica, "Open Sans", sans-serif;
  font-weight:500;
  font-style:italic
}

.pager li > a,.pager li > span
{
  border-radius:.5em
}

#self
{
  -moz-border-radius:100px;
  -webkit-border-radius:100px;
  border-radius:10px;
  max-height:200px;
  margin:0;
  padding:10px
}

#disqus_thread
{
  margin-top:100px
}

.recent-posts
{
  background-color:#EAEAEF ;
  color:#000000;
  margin:0;
  padding-right:10%;
  line-height:30px;
  width:100%;
}


.recent-posts h1
{
  padding:0 2.7%;
  font-size:1.5em
}

.project-container
{
  width:85%;
  height:140px;
  box-shadow:inset 0 -10px 5px -2px rgba(119,119,119,0.35);
  -moz-box-shadow:inset 0 -10px 5px -2px rgba(119,119,119,0.35);
  -webkit-box-shadow:inset 0 -10px 5px -2px rgba(119,119,119,0.35);
  border-bottom:solid 1px #fff;
  background-image:url(http://lorempixel.com/900/200/);
  background-size: cover;
  z-index:-4;
  margin-top:-55px;
  margin-bottom:30px;
  margin-left:80px;
  color:#fff
}
.people-container
{
  width:90%;
  height:140px;
  box-shadow:inset 0 -10px 5px -2px rgba(119,119,119,0.35);
  -moz-box-shadow:inset 0 -10px 5px -2px rgba(119,119,119,0.35);
  -webkit-box-shadow:inset 0 -10px 5px -2px rgba(119,119,119,0.35);
  border-bottom:solid 1px #fff;
  background-image:url(http://lorempixel.com/900/200/nature);
  background-size: cover;
  z-index:-4;
  margin-top:-55px;
  margin-bottom:30px;
  color:#fff
}

.people
{ width:90%;
	}
.project-container h1
{
  font-size:3em;
  font-family:"Helvetica Neue", helvetica, "Open Sans", sans-serif;
  font-weight:bolder;
  color:#fff;
  text-align:center;
  padding-top:20px
}


.people-container h1
{
  font-size:2.5em;
  font-family:"Helvetica Neue", helvetica, "Open Sans", sans-serif;
  font-weight:bolder;
  color:#fff;
  text-align:center;
  padding-top:40px
}

.introduction
{padding-left:10px;
 padding-right:10px;
 padding-bottom:120px
	
}


.project-container h1
{
  font-weight:200;
  font-family:"Helvetica Neue", helvetica, "Open Sans",sans-serif;
  color:#fff;
  text-align:center;
  left-padding:50%
}

.project-box
{
  background-color:#f9f9f9;
  box-shadow:1px 1px 0 1px #e8e8e8;
  min-height:200px
}

.project-box:hover
{
  background-color:#fff;
  box-shadow:1px 1px 0 1px #e8e8e8;
  min-height:200px
}

.project-title
{
  color:#000
}

.project-image
{
  position:relative;
  margin:0
}

.project-image hr
{
  margin-top:5px;
  margin-bottom:0;
  display:none
}

.project-post
{
  cursor:pointer
}

.project-post h4
{
  font-weight:200;
  font-size:1em
}

.user-card
{
  background-color:#f9f9f9;
  box-shadow:1px 1px 0 1px #e8e8e8;
  max-width:800px;
  padding:10px 40px 0
}

.user-card h1
{
  font-family:"Helvetica Neue", helvetica, "Open Sans",sans-serif;
  font-weight:bolder;
  font-size:2.5em;
  line-height:55px;
  color:#d90006
}

.user-card p
{
  color:#c4c4c4;
  font-size:small
}

.card
{
  margin-left:-40px;
  margin-top:-10px
}

.letter p
{
  font-size:1.2em;
  font-weight:lighter;
  color:#737373;
  text-shadow:1px 1px #fff
}

.fashion
{
  border-left:solid 2px #bc0005
}

.fashion-button
{
  background-color:#7dbcb2
}

.fashion:hover
{
  border-left:solid 2px #bc0005
}

.fashion-button:hover
{
  background-color:#8B1A1A
}

.programming
{
  border-left:solid 2px #EE7600
}

.programming-button
{
  background-color:#7dbcb2
}

.programming:hover
{
  border-left:solid 2px #644539
}

.programming-button:hover
{
  background-color:#8B1A1A
}

.tech
{
  border-left:solid 2px #644539
}

.tech-button
{
  background-color:#7dbcb2
}

.tech:hover
{
  border-left:solid 2px #EEEE00
}

.tech-button:hover
{
